{
    "family": "af-front",
    "executionRoleArn": "arn:aws:iam::430278725858:role/ecsTaskExecutionRole",
    "networkMode": "bridge",
    "containerDefinitions": [
        {
            "name": "af-front",
            "image": "public.ecr.aws/i7k0q0b2/af-front:latest",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "256",
    "memory": "256"
}